{
  "*name": "string",
  "*tagline": "string",
  "*company": "string",
  "*companyUrl": "string",
  "*sections": [
    {
      "oneOf": [
        {
          "type": "navbar-1"
        },
        {
          "type": "footer-1",
          "text": "string"
        },
        {
          "type": "hero-signup-image-half-1",
          "*titleText": "string",
          "subtitleText": "string",
          "logoImageUrl": "string",
          "inputType": {
            "type": "string",
            "options": ["email", "url"],
            "default": "email"
          },
          "inputPlaceholderText": "string",
          "inputButtonText": "string",
          "inputSuccessMessageText": "string",
          "inputName": "string",
          "formTarget": "string",
          "formAction": "string"
        }
      ]
    }
  ]
}

